if BOARD_ACN52832

config BOARD
	default "acn52832"

if GPIO_NRF5

config GPIO_NRF5_P0
	default y

endif # GPIO_NRF5

if UART_NRF5

config UART_NRF5_GPIO_TX_PIN
	default 26

config UART_NRF5_GPIO_RX_PIN
	default 27

config UART_NRF5_GPIO_RTS_PIN
	default 25

config UART_NRF5_GPIO_CTS_PIN
	default 28

endif # UART_NRF5

endif # BOARD_ACN52832
